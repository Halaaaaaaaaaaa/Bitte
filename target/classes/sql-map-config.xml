<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<properties resource="database.properties">
	</properties>
	<typeAliases>
		<typeAlias type="com.bitte.biz.dto.UserVO" alias="User"/>
		<typeAlias type="com.bitte.biz.dto.TotalShopVO" alias="Total_shop"/>
		<typeAlias type="com.bitte.biz.dto.ProductColorVO" alias="Product_color"/>
		<typeAlias type="com.bitte.biz.dto.ProductSizeVO" alias="Product_size"/>
		<typeAlias type="com.bitte.biz.dto.ShopVO" alias="Shop"/>
		<typeAlias type="com.bitte.biz.dto.ReviewVO" alias="Review"/>
		<typeAlias type="com.bitte.biz.dto.WishListVO" alias="Wish"/>
	</typeAliases>
	
	<!-- sql 매핑 파일 등록 -->
	<mappers>
		<mapper resource="mappings/user-mapping.xml"/>
		<mapper resource="mappings/total_shop-mapping.xml"/>
		<mapper resource="mappings/review-mapping.xml"/>
		<mapper resource="mappings/wishList-mapping.xml"/>
	</mappers>
</configuration>
